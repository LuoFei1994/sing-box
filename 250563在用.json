                "server": "Local-DNS"
            },

            {
                "rule_set": [
                    "geosite-geolocation-!cn"
                ],
                "server": "Remote-DNS"
            },


            // 4. 兜底规则（最后）
            {
                "query_type": [
                    "A",
                    "AAAA"
                ],
                "server": "fakeip"
            },
            {
                "query_type": [
                    "HTTPS",
                    "SVCB",
                    "TXT",
                    "MX",
                   "CNAME"
               ],
               "server": "Raw-DNS",  // TCP的dns支持SRV等特殊查询
               "disable_cache": true
            }
            
