        "tag": "local",
        "type": "local"
        //"prefer_go": true
      },
      {
        "tag": "fake-ip-dns-server",
        "type": "fakeip",
        "inet4_range": "198.18.0.0/15",
        "inet6_range": "fc00::/18"
      },
      {
        "tag": "hosts",
        "type": "hosts",
        "predefined": {
          "dns.alidns.com": [
            "223.5.5.5",
            "223.6.6.6",
            "2400:3200::1",
            "2400:3200:baba::1"
          ],
          "dns.google": [
            "8.8.8.8",
            "8.8.4.4"
          ]
        }
      }
    ],
    "rules": [

      {
        "clash_mode": "Direct",
        "action": "route",
        "server": "local"
      },
      {
        "clash_mode": "Global",
        "action": "route",
        "server": "google"
      },

      {
        "query_type": ["ANY","HTTPS", "SVCB", "NAPTR", "SOA", "NS", "CAA", "OPT"],
        "action": "reject"
      },
      
      {
        "query_type": [
          "A",
          "AAAA"
        ],
        "action": "route",
        "server": "fake-ip-dns-server",
        "rewrite_ttl": 5
      },
      {
        "query_type": [
          "CNAME", 
          "TXT", 
          "MX", 
          "SRV"
        ],
        "action": "route",
        "server": "google"
      },
      { 
        "rule_set": [
          "category-ads-all", 
          "tracker"
        ], 
        "action": "reject" 
      }
    ],
    "cache_capacity": 4096,  
    "reverse_mapping": true,
    "disable_cache": false,
    "disable_expire": false,
    "independent_cache": true,
    "final": "google",
    "strategy": "ipv4_only"
  },
