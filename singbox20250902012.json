{
  "dns": {
    "servers": [
      {
        "tag": "ali",
        "type": "https",
        "server": "dns.alidns.com",
        "domain_resolver": "hosts"
        //"detour": "direct"
      },
      {
        "tag": "google",
        "type": "https",
        "server": "dns.google",
        "domain_resolver": "hosts",
        "detour": "节点"
      },
      {
        "tag": "local",
        "type": "local"
      },
      {
        "tag": "fake-ip-dns-server",
        "type": "fakeip",
        "inet4_range": "198.18.0.0/15",
        "inet6_range": "fc00::/18"
      },
      {
        "tag": "hosts",
        "type": "hosts",
        "predefined": {
          "dns.alidns.com": [
            "223.5.5.5",
            "223.6.6.6",
            "2400:3200::1",
            "2400:3200:baba::1"
          ],
          "dns.google": [
            "8.8.8.8",
            "8.8.4.4"
          ]
        }
      }
    ],
    "rules": [
      {
        "clash_mode": "Direct",
        "action": "route",
        "server": "local"
      },
      {
        "clash_mode": "Global",
        "action": "route",
        "server": "fake-ip-dns-server"
      },
      /*{
        "rule_set": "AWAvenue-Ads",
        "action": "reject",
        "rcode": "NXDOMAIN"
      },*/
      {
        "action": "reject",
        "query_type": [
          "HTTPS"
        ]
      },
      {
        "rule_set": [
          "binance"
        ],
        "action": "route",
        "server": "ali"
      },
      {
        "query_type": [
          "A",
          "AAAA"
        ],
        "action": "route",
        "server": "fake-ip-dns-server",
        "rewrite_ttl": 1
      },
      {
        "rule_set": "cn",
        "action": "route",
        "server": "ali"
      }
    ],
    "final": "google",
    "independent_cache": true
  },
  "route": {
    "rules": [
      {
        "action": "sniff",
        "sniffer": [
          "http",
          "tls",
          "quic",
          "dns"
        ],
        "timeout": "500ms"
      },
      {
        "type": "logical",
        "mode": "and",
        "rules": [
          { "network": "udp" },
          { "port": 53 }
        ],
        "action": "hijack-dns"
      },
      {
        "clash_mode": "Direct",
        "action": "route",
        "outbound": "direct"
      },
      {
        "clash_mode": "Global",
        "action": "route",
        "outbound": "节点"
      },
      /*{
        "rule_set": "AWAvenue-Ads",
        "action": "reject"
      },*/

    
      {
        "ip_is_private": true,
        "action": "route",
        "outbound": "direct"
      },
      {
        "rule_set": [
          "private",
          "binance"
        ],
        "action": "route",
        "outbound": "direct"
      },
      
      {
        "rule_set": [
          "telegram",
          "github",
          "twitter",
          "youtube",
          "google",
          "gfw"
        ],
        "action": "route",
        "outbound": "节点"
      },
      {
        "action": "resolve"
        //"match_only": true
      },

      {
        "ip_cidr": ["198.18.0.0/15", "fc00::/7"],
        "action": "reject"
      },
      
      {
        "ip_cidr": [
          "122.248.193.92/32",
          "18.140.160.130/32",
          "13.228.58.214/32",
          "18.136.221.104/32",
          "13.214.82.8/32",
          "13.228.179.43/32"
        ],
        "outbound": "direct"
      },
      {
        "rule_set": "cn_ip",
        "action": "route",
        "outbound": "direct"
      }
    ],
    //下面默认dns是给节点使用的
    "default_domain_resolver": {
      "server": "ali",
      "strategy": "ipv4_only",
      "rewrite_ttl": 120
