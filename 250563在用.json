                "client_subnet": "0.0.0.0/0"
            },
            
            //====clash mode的规则=====
            {
                "action": "route",
                "clash_mode": "direct",
                "server": "Local-DNS"
            },
            {
                "action": "route",
                "clash_mode": "global",
                "server": "Remote-DNS"
            },

            // 3. 区域分流规则
            {
                "rule_set": [
                    "GeoSite-CN"
